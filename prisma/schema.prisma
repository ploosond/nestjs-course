generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

enum Role {
  INTERN 
  ENGINEER 
  ADMIN
}

model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String @unique
  name  String
  role  Role
  createAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
